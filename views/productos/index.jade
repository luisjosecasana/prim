extends ../layout.jade
block contenido
	h2(class='pacifico big blue') Menú
	p Adquisición de datos para las Ramas de la Medicina
	each product in products
		div(class="col-md-4 col-sm-6 col-xs-12 inline-block")
			h3(class="pacifico")= product.title
			img(src="#{ product.image.url }" class="col-xs-7 col-md-4 quitar-float")
			p( style="margin-top:1em;") #[small(class="lato small") #{product.description}Detalles]
